{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": null,
  "categories": {},
  "rules": {
		"eslint/no-unused-vars": ["warn", { "caughtErrors": "none", "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }],
		"eslint/no-cond-assign": "off",
		"eslint/no-unused-expressions": "off",
		"eslint/no-constant-condition": "off",
		"eslint/no-loss-of-precision": "off",
		"unicorn/prefer-string-starts-ends-with": "off",
		"unicorn/no-new-array": "off",
		"unicorn/no-useless-spread": "off",
		"unicorn/no-thenable": "off"
	},
	"overrides": [
		{
			"files": ["tests/**/*.js"],
			"rules": {
				"eslint/no-unused-vars": "off"
			}
		}
	],
  "settings": {
    "jsx-a11y": {
      "polymorphicPropName": null,
      "components": {},
      "attributes": {}
    },
    "next": {
      "rootDir": []
    },
    "react": {
      "formComponents": [],
      "linkComponents": [],
      "version": null,
      "componentWrapperFunctions": []
    },
    "jsdoc": {
      "ignorePrivate": false,
      "ignoreInternal": false,
      "ignoreReplacesDocs": true,
      "overrideReplacesDocs": true,
      "augmentsExtendsReplacesDocs": false,
      "implementsReplacesDocs": false,
      "exemptDestructuredRootsFromChecks": false,
      "tagNamePreference": {}
    },
    "vitest": {
      "typecheck": false
    }
  },
  "env": {
    "builtin": true,
    "node": true,
    "mocha": true
  },
  "globals": {},
  "ignorePatterns": [
    "/node_modules",
    "**/locales/**/*.js",
    "android/runtime/v8/tools/bootstrap.js",
    "android/dev",
    "android/modules/ui/assets/Resources/ti.internal/webview/*.js",
    "android/titanium/build",
    "android/**/generated/",
    "iphone/Resources/app.js",
    "templates/**",
    "tests/fake_node_modules/",
    "tests/node_modules/",
    "tests/Resources/node_modules/",
    "dist/",
    "tmp/"
  ]
}

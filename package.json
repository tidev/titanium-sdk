{
  "name": "titanium-mobile",
  "description": "Appcelerator Titanium Mobile",
  "version": "8.2.1",
  "moduleApiVersion": {
    "iphone": "2",
    "android": "4",
    "windows": "7"
  },
  "keywords": [
    "appcelerator",
    "titanium",
    "mobile",
    "android",
    "iphone",
    "ipad",
    "ios"
  ],
  "author": "Appcelerator, Inc. <npmjs@appcelerator.com>",
  "scripts": {
    "build:docs": "docgen apidoc",
    "build:changelog": "conventional-changelog -n changelog/config.js -i CHANGELOG.md -s -p angular",
    "test": "npm run ios-sanity-check && grunt",
    "android": "./build/scons cleanbuild android",
    "android-test": "./build/scons clean android && ./build/scons build android && ./build/scons package -s --no-docs android && ./build/scons install -s && ti sdk select $npm_package_version && ./build/scons test -s android",
    "ios": "./build/scons cleanbuild ios",
    "ios-sanity-check": "./build/scons check-ios-toplevel",
    "ios-test": "./build/scons clean ios && ./build/scons build ios && ./build/scons package -s --no-docs ios && ./build/scons install -s && ti sdk select $npm_package_version && ./build/scons test -s ios",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "iphone/**/*.{m,h}": [
      "npx clang-format -style=file -i",
      "git add"
    ],
    "iphone/TitaniumKit/TitaniumKit/Sources/API/TopTiModule.m": [
      "npm run ios-sanity-check --"
    ],
    "*.java": [
      "npx clang-format -style=file -i",
      "git add"
    ],
    "!(**/locales/**/*).js": "eslint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.11",
    "appc-aar-tools": "^1.1.5",
    "appc-tasks": "^1.0.2",
    "archiver": "^3.1.1",
    "async": "^2.6.1",
    "buffer-equal": "1.0.0",
    "clean-css": "4.2.1",
    "colors": "^1.3.3",
    "css-parse": "2.0.0",
    "ejs": "^2.6.1",
    "fields": "0.1.24",
    "fs-extra": "^8.0.1",
    "ioslib": "^1.7.18",
    "klaw-sync": "^6.0.0",
    "liveview": "^1.5.0",
    "lodash.defaultsdeep": "^4.6.1",
    "markdown": "0.5.0",
    "moment": "^2.22.2",
    "node-appc": "^0.3.4",
    "node-titanium-sdk": "^3.2.1",
    "node-uuid": "1.4.8",
    "nodeify": "^1.0.1",
    "p-limit": "^2.2.0",
    "pngjs": "^3.4.0",
    "request": "^2.87.0",
    "semver": "^6.3.0",
    "sprintf": "0.1.5",
    "temp": "0.9.0",
    "windowslib": "^0.6.8",
    "xcode": "^2.0.0",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@seadub/danger-plugin-dependencies": "0.1.0",
    "@seadub/danger-plugin-eslint": "^1.0.1",
    "@seadub/danger-plugin-junit": "0.1.1",
    "clang-format": "1.2.3",
    "commander": "^3.0.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.25",
    "core-js": "^2.6.5",
    "cz-conventional-changelog": "^3.0.2",
    "danger": "^9.1.8",
    "dateformat": "^3.0.3",
    "eslint": "^6.2.2",
    "eslint-config-axway": "^4.3.0",
    "eslint-plugin-mocha": "^6.1.0",
    "glob": "^7.1.4",
    "grunt": "^1.0.4",
    "grunt-clang-format": "^1.1.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-eslint": "^22.0.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ssri": "^6.0.1",
    "titanium-docgen": "^0.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/appcelerator/titanium_mobile.git"
  },
  "vendorDependencies": {
    "visual studio": ">=10 <=12",
    "windows phone sdk": "8.x",
    "msbuild": ">=4.0",
    "node": "8.x || 10.x"
  },
  "engines": {
    "node": ">=8"
  },
  "license": "Apache-2.0"
}
